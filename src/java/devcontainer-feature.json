{
  "name": "My Favorite Color",
  "id": "java",
  "version": "1.0.3",
  "description": "A feature to remind you of your favorite color",
  "options": {
      "favorite": {
          "type": "string",
          "enum": [
              "red",
              "gold",
              "green"
          ],
          "default": "red",
          "description": "Choose your favorite color."
      },
      "version": {
          "type": "string",
          "enum": [
              "11",
              "16",
              "8",
              "17",
              "21"
          ],
          "default": "11",
          "description": "Choose the version of Java to install."
      }
  },
  "installsAfter": [
      "ghcr.io/devcontainers/features/common-utils"
  ]
}